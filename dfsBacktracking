#include <bits/stdc++.h>
#define MAX 10000
using namespace std;
bool v[MAX][MAX];
char ad[MAX][MAX];
pair<int,int> sig;
int n,m,x,y;
int dir[2][4] = { {1,-1,0,0},{0,0,1,-1} };

void dfs(pair<int,int> a){
    v[a.first][a.second] = true;

    if(ad[a.first][a.second] == 'S')
        cout << a.first << ' ' << a.second << endl;

    for(int i=0;i<4;i++){
        sig.first = a.first + dir[0][i];
        sig.second = a.second + dir[1][i];

        if( (sig.first > 0 && sig.first <= n) && (sig.second > 0 && sig.second <= m) )
            if(!v[sig.first][sig.second] && ad[sig.first][sig.second] != '#')
                dfs(sig);
    }
}

int main(){
    ifstream fin("in.txt");

    fin >> n >> m >> x >> y;

    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            fin >> ad[i][j];
            v[i][j] = false;
        }
    }

    dfs(make_pair(x,y));
}
